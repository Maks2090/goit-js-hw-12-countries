{{#each this}}
<h1>{{name}}</h1>
<div>
    <div>
        <p>Capital: {{capital}}</p>
        <p>Population: {{population}}</p>
        <p>Languages:
        <ul>
            {{#each languages}}
            <li>{{nativeName}}</li>
            {{/each}}
        </ul>
        </p>
    </div>
    <div>
        <img src="{{flag}}" alt="flag {{name}}" height="200px" width="350px">
    </div>
</div>
{{/each}}